<templateSet group="AndroidComments">
  <template name="classhead" value="$end$&#10;* &#10;* @author by SunYuXing on $date$." description="add a class file header" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
